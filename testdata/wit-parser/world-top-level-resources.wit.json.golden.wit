package foo:foo;

interface handler {
	use types.{request};
	use types.{response};
	handle: func(some: borrow<request>) -> response;
	handle-owned: func(some: request) -> response;
}

interface types {
	resource request {
		bar: func(arg: list<u32>);
		foo: func();
	}
	resource response {
		bar: func(arg: list<u32>);
		foo: func();
	}
}

world proxy {
	import handler;
	import types;
	export handler;
}
