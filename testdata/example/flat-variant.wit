package example:flat-variant;

interface a {
	variant numbers {
		one,
		two(string),
		three(float64),
		four(option<string>),
	}

	f0: func(f: float64);

	f1: func(v: numbers);

	type optional-float = option<float64>;

	variant in32 {
		one,
		two(s32),
		three(u32),
		four(f32),
	}

	f2: func(v: in32);
}

world imports {
	import a;
	export a;
}
